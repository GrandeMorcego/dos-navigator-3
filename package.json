{
  "name": "dn3",
  "version": "0.1.1",
  "description": "Dos Navigator III",
  "main": "./src/electron-main.js",
  "author": "Grande Morcego",
  "license": "MIT",
  "scripts": {
    "start-dev": "webpack-dev-server --port 8888",
    "build": "webpack --config webpack.config.js",
    "build-prod": "webpack --config webpack.config.prod.js",
    "start": "electron-forge start",
    "electron": "export NODE_ENV=dev && electron . --inspect=8989",
    "package": "export NODE_ENV=prod && electron-forge package",
    "make": "export NODE_ENV=prod && electron-forge make",
    "electron-win": "set NODE_ENV=dev && electron-forge start",
    "package-win": "set NODE_ENV=prod && electron-forge package",
    "make-win": "set NODE_ENV=prod && electron-forge make",
    "publish": "electron-forge publish",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "build": {
    "appId": "com.electron.${name}",
    "productName": "Dos Navigator III",
    "directories": {
      "buildResources": "build",
      "output": "output"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "icon": "icons/icons/mac/icns"
    },
    "win": {
      "target": "msi",
      "icon": "icons/icons/win/icon.ico"
    },
    "linux": {
      "target": "deb",
      "icon": "icons/icons/png/64x64.png",
      "category": "public.app-category.developer-tools"
    }
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "icon": "./icons/icons/mac/icon.icns",
        "packageManager": "yarn",
        "asar": false,
        "ignore": [
          "=src/index.js",
          "=src/Main.js",
          "=src/common/",
          "=src/system/",
          "=src/drives/",
          "build/.*map",
          "=src/__files/"
        ]
      },
      "electronWinstallerConfig": {
        "setupIcon": "./icons/icons/win/icon.ico",
        "name": "Dos Navigator III"
      },
      "electronInstallerDebian": {
        "setupIcon": "./icons/icons/png/64x64.png"
      },
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "GrandeMorcego",
        "name": "dos-navigator-3"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "Dos Navigator III"
      }
    }
  },
  "dependencies": {
    "drivelist": "^6.3.0",
    "electron-compile": "^6.4.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^2.0.0",
    "express": "^4.16.3",
    "express-ws": "^3.0.0",
    "ncp": "^2.0.0",
    "node-pty": "^0.7.4",
    "opn": "^5.3.0",
    "path": "^0.12.7",
    "rimraf": "^2.6.2",
    "semver": "^5.4.0",
    "tls": "^0.0.1",
    "trash": "^4.3.0",
    "usb-detection": "^3.2.0",
    "xterm": "^3.3.0"
  },
  "devDependencies": {
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "axios": "^0.16.2",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.11",
    "electron": "^2.0.5",
    "electron-builder": "^20.13.3",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.4",
    "electron-wix-msi": "^1.3.0",
    "eslint": "^3",
    "eslint-config-airbnb": "^15",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7",
    "file-loader": "^1.1.11",
    "flexband": "^0.0.3",
    "html-webpack-plugin": "^2.28.0",
    "ln3": "^0.0.1",
    "monaco-editor": "0.13.1",
    "monaco-editor-webpack-plugin": "^1.4.0",
    "node-pre-gyp": "^0.10.3",
    "observed-object": "^0.1.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-addons-transition-group": "^15.6.2",
    "react-dom": "^16.3.2",
    "react-monaco-editor": "^0.17.2",
    "react-onscroll": "^1.0.4",
    "react-reflex": "^2.2.4",
    "react-scroll-listener": "^0.6.0",
    "react-tap-event-plugin": "^3.0.2",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^1.0.1",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2"
  }
}
